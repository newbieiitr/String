#include <iostream>
using namespace std;

void Add_per(vector <string> &sol,char c,int i,unordered_map<string, int> &hash) {
    
    for(int j=0;j<sol[i].length();j++){
        string s1="";
        for(int k=0;k<sol[i].length();k++){
            if(k==j){
                s1+=sol.[i].at(k);
                s1+=c;
                
            }
            else {
                s1+=sol.[i].at(k);
            }
        }
        sol.push_back(s1);
    }
}

vector<vector <string> > per (string &s){
    
    //initialisation
    vector <string> sol;
    vector <string> temp;
    string str="";
    str+=s.at(0);
    sol.push_back(str);
    
    for(int i=1;i<s.length();i++){
int size=sol.size();
char c=s.at(i);
unordered_map<string, int> hash;
        
         for(int i=0;i<size;i++){
             Add_per(sol,c,i,hash);
             // add char every where except before string and push_back them to sol
         }
         for(int i=0;i<size;i++){
             string s1=c+sol[i];
             if (hash.find(s1) == hash.end()) {
                 sol[i]=s1;
                 hash[s1]=1;
             }
             else{
                 sol.erase (sol.begin()+i);
             }
             
         }
         
         
        
    }
    
}

