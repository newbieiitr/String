std::vector<string> arr(string &s){
    string temp="";
    vector <string> V;
    s=s+' ';
//just to inialise comparing
    int count=0;
    for(int i=0;i<s.length();i++){
        if(s.at(i)==' '){   
            if(i!=0){
                if(s.at(i-1)!=' '   ) {
                    V.push_back(temp);
                    count++;
                    temp="";
            }
            
        } }
        else {
            temp+=s.at(i);
        }
    }
    return V;
}
